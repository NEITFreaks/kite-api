const express = require('express')
const app = express()
const port = 3000;

app.get('/', (req, res) => res.send('Hello World!'));

var router = express.Router();

app.use('/api', router)

router.get('/test', (req,res)=>{
    res.send("Test")
});

app.listen(port, () => console.log(`Example app listening on port ${port}!`))